{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dashboard/$1",
      "has": [
        {
          "type": "host",
          "value": "app.solaiexp.vercel.app"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        }
      ]
    }
  ]
}